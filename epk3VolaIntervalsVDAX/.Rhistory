library(caTools)
toIntegrate <- function(x){
(dnorm(x)*(1-x^2))^2
}
toIntegrate(seq(-100, 100, length.out = 1000))
trapz(seq(-1000, 1000, length.out = 10000),
toIntegrate(seq(-1000, 1000, length.out = 10000)))
